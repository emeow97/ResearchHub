<Page x:Class="ResearchHub.AddList"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ResearchHub"
      mc:Ignorable="d" 
      d:DesignHeight="880" d:DesignWidth="1400"
      Title="AddList">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="source file/Add a Listing.png"/>
        </Grid.Background>
        <Button Content="Button" HorizontalAlignment="Left" Margin="80,65,0,0" VerticalAlignment="Top" Width="50" Height="50" BorderBrush="{x:Null}" Click="Button_Click">
            <Button.Background>
                <ImageBrush ImageSource="source file/Back button.png"/>
            </Button.Background>
        </Button>
    </Grid>
</Page>
